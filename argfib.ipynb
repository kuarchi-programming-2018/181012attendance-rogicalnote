    "def fib(n):\n",
    "    if n > 1:\n",
    "        return fib(n-1) + fib(n-2)\n",
    "    else:\n",
    "        return 1\n",
    "print(fib(7))\n",
    "\n",
    "def argfib(m):\n",
    "    for i in i-=1:\n",
    "        \n",
    "        if fib(i)<=m:\n",
    "            return fib(i)\n",
    "        else:\n",
    "            return m\n",
    "print(argfib(2018))"
